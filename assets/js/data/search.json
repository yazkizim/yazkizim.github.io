---
layout: compress
swcache: true
---

[
  {% for teskilat in site.data.teskilat %}

  {
    "title": {{ teskilat.mahal | jsonify }} " Adliyesi Nereye Bağlı?",
    "url": "{{ '/adliye/?adliye=' | append: teskilat.mahal }}",
    "categories": "Adli Teşkilat",
    "tags": "Adli Teşkilat",
    "date": "{{ now }}",
    {% include no-linenos.html content= sozluk.anlami %}
    {% assign _content = content | strip_html | strip_newlines %}
    "snippet": "Detaylar için tıklayınız",
    "content": "Detaylar için tıklayınız"
  }{% unless forloop.last %},{% endunless %}
  {% endfor %},

  {% for post in site.posts %}
  {
    "title": {{ post.title | jsonify }},
    "url": {{ post.url | relative_url | jsonify }},
    "categories": {{ post.categories | join: ', ' | jsonify }},
    "tags": {{ post.tags | join: ', ' | jsonify }},
    "date": "{{ post.date }}",
    {% include no-linenos.html content=post.content %}
    {% assign _content = content | strip_html | strip_newlines %}
    "snippet": {{ _content | truncate: 200 | jsonify }},
    "content": {{ _content | jsonify }}
  }{% unless forloop.last %},{% endunless %}
  {% endfor %},

  {% for sozluk in site.data.sozluk %}

  {
    "title": {{ sozluk.kelime | jsonify }},
    "url": "{{ '/sozluk/?kelime=' | append: sozluk.kelime }}",
    "categories": "Sözlük",
    "tags": "Sözlük",
    "date": "{{ now }}",
    {% include no-linenos.html content= sozluk.anlami %}
    {% assign _content = content | strip_html | strip_newlines %}
    "snippet": {{ sozluk.anlami | truncate: 200 | jsonify }},
    "content": {{ sozluk.anlami | jsonify }}
  }{% unless forloop.last %},{% endunless %}
  {% endfor %}
]
